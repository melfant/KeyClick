<AnchorPane prefHeight="170.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChangePeriodsNumberController">
   <children>
      <VBox alignment="CENTER" layoutX="26.0" layoutY="22.0" prefHeight="45.0" prefWidth="260.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="22.0">
         <children>
            <Label fx:id="lbEnterKey" alignment="CENTER" prefHeight="80.0" prefWidth="239.0" text="Enter periods number">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="tfPeriodsNumber" alignment="CENTER_RIGHT">
               <VBox.margin>
                  <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="bnSave" onAction="#savePeriodsNumber" disable="true" mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" text="Save">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
